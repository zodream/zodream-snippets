            "<!-- {if ${1:$$data eq ''}} -->",
            "${2:Alert body ...}",
            "<!-- {/if} -->"
        ],
		"description": "if 判断",
		"scope": ""
    },
    "s-if:else": {
		"prefix": "s-if:else",
		"body": [
            "<!-- {if ${1:$$data eq ''}} -->",
            "${2:Alert body ...}",
